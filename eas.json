{
  "cli": {
    "version": ">= 7.3.0",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "channel": "staging",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "staging",
        "EXPO_PUBLIC_BACKEND_URL": "https://qa."
      }
    },
    "production": {
      "channel": "main",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_BACKEND_URL": "https://prod"
      }
    }
  },
  "submit": {
